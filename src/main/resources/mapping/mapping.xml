        <field>
            <a>userVO</a>
            <b>userWVO</b>
        </field>
        <field>
            <a>companyVO</a>
            <b>companyWVO</b>
        </field>
    </mapping>

    <mapping>
        <class-a>com.example.Invoice.model.VO.ClientVO</class-a>
        <class-b>com.example.Invoice.model.WVO.ClientWVO</class-b>
        <field>
            <a>userVO</a>
            <b>userWVO</b>
        </field>
    </mapping>


    <mapping>
        <class-a>com.example.Invoice.model.VO.CompanyVO</class-a>
        <class-b>com.example.Invoice.model.WVO.CompanyWVO</class-b>
