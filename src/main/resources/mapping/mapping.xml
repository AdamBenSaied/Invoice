            <b>cashierWVO</b>
        </field>

    </mapping>

    <mapping>
        <class-a>com.example.Invoice.model.VO.ProductVO</class-a>
        <class-b>com.example.Invoice.model.WVO.ProductWVO</class-b>
    </mapping>

    <mapping>
        <class-a>com.example.Invoice.model.VO.UserVO</class-a>
        <class-b>com.example.Invoice.model.WVO.UserWVO</class-b>
